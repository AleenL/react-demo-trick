.background{
  width: 100%;
  height: 100vh;
  background-repeat: no-repeat;
  background-size: cover;
  background-position: center;
  background-blend-mode: multiply;
  &.afternoon{
    background-image: url('../../../public/image/Sun.png');
  }
  &.morning{
    background-image: url('../../../public/image/Moon.png');
  }
  &.night{
    background-image: url('../../../public/image/Blood.png');
  }
}